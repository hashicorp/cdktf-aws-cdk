{
  "name": "@cdktf/aws-cdk",
  "description": "Adapter for using AWS CDK constructs in Terraform CDK (cdktf) projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashicorp/cdktf-aws-cdk.git"
  },
  "scripts": {
    "build": "npx projen build",
    "bump": "npx projen bump",
    "clobber": "npx projen clobber",
    "compat": "npx projen compat",
    "compile": "NODE_OPTIONS=\"--max-old-space-size=7516\" jsii --silence-warnings=reserved-word",
    "default": "npx projen default",
    "docgen": "npx projen docgen",
    "eject": "npx projen eject",
    "eslint": "npx projen eslint",
    "examples:test": "npx projen examples:test",
    "examples:test:update": "npx projen examples:test:update",
    "examples:update": "npx projen examples:update",
    "package": "npx projen package",
    "package-all": "npx projen package-all",
    "package:js": "npx projen package:js",
    "post-compile": "npx projen post-compile",
    "post-upgrade": "npx projen post-upgrade",
    "pre-compile": "npx projen pre-compile",
    "release": "npx projen release",
    "test": "jest",
    "test:watch": "npx projen test:watch",
    "unbump": "npx projen unbump",
    "upgrade": "npx projen upgrade",
    "watch": "npx projen watch",
    "projen": "npx projen",
    "fetch:types": "mkdir -p src/awscc && rm -rf ./src/awscc/* && node ./scripts/update-supported-types.js",
    "fetch": "mkdir -p src/aws && rm -rf ./src/aws/* && cdktf get && cp -R .gen/providers/aws/* ./src/aws/ && mkdir -p src/time && rm -rf ./src/time/* && cp -R .gen/providers/time/* ./src/time/",
    "commit": "git add -A && git commit -am \"Update provider\" || echo \"No changes to commit\"",
    "should-release": "! git diff --exit-code v$(cat version.json | jq -r '.version') ./src ./package.json",
    "prebump": "yarn fetch && yarn compile && yarn run commit && yarn run should-release",
    "build-provider": "yarn fetch && yarn compile && yarn docgen"
  },
  "author": {
    "name": "HashiCorp",
    "url": "https://hashicorp.com",
    "organization": true
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.36.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "aws-cdk-lib": "2.80.0",
    "cdktf": "0.20.0",
    "cdktf-cli": "0.20.0",
    "commit-and-tag-version": "^12",
    "constructs": "10.0.25",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29",
    "jest-junit": "^16",
    "jsii": "~5.5.0",
    "jsii-diff": "^1.107.0",
    "jsii-docgen": "^10.6.7",
    "jsii-pacmak": "^1.107.0",
    "jsii-rosetta": "~5.5.0",
    "prettier": "^2.8.8",
    "projen": "^0.91.13",
    "ts-node": "^10.9.1",
    "typescript": "~5.5.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.80.0",
    "cdktf": "^0.20.0",
    "constructs": "^10.0.25"
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "codemaker": "^1.34.0",
    "debug": "^4.3.2"
  },
  "bundledDependencies": [
    "change-case",
    "codemaker",
    "debug"
  ],
  "resolutions": {
    "cookie": "0.7.0",
    "cross-spawn": "7.0.6",
    "cssstyle": "4.1.0",
    "follow-redirects": "1.15.6"
  },
  "keywords": [
    "aws",
    "aws-cdk",
    "cdk",
    "cdktf",
    "terraform"
  ],
  "engines": {
    "node": ">= 18.12.0"
  },
  "main": "lib/index.js",
  "license": "MPL-2.0",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "jest": {
    "coverageProvider": "v8",
    "setupFilesAfterEnv": [
      "./setupJest.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/examples",
      ".yalc",
      ".+\\.d\\.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/examples",
      ".yalc",
      ".+\\.d\\.ts",
      "lib/aws",
      "lib/time"
    ],
    "testMatch": [
      "<rootDir>/@(lib/tests)/**/*(*.)@(spec|test).js?(x)",
      "<rootDir>/@(lib/tests)/**/__tests__/**/*.js?(x)",
      "<rootDir>/@(projenrc)/**/*(*.)@(spec|test).ts?(x)",
      "<rootDir>/@(projenrc)/**/__tests__/**/*.ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "watchPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "snapshotResolver": "./.projen/jest-snapshot-resolver.js"
  },
  "types": "lib/index.d.ts",
  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "targets": {},
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
